{
    "version": 2,
    "builds": [
      {
        "src": "./server/server.js", 
        "use": "@vercel/node"
      },
      {
        "src": "./products/build",    
        "use": "@vercel/static"
      }
    ],
  
   
  
    "rewrites": [
      {
        "source": "/(.*)",
        "destination": "/server/server.js"
      }
    ]
  }